<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>

  <appSettings>
    <add key="baseCurrency" value="EUR"/>

    <!-- lähtetetäänkö lasketut tapahtumat oikeasti terminaaliin (true/false) -->
    <add key="sendTransaction" value="false"/>
    
    <!-- value = pilkuilla eroteltu lista, oltava sama maara kaikissa seuraavassa neljassa -->
    <add key="securities1" value="SPY US EQUITY" />
    <add key="securities2" value="AAPL 150117C00600000 EQUITY" />
    <add key="currencies1" value="USD"/>
    <add key="currencies2" value="EUR"/>
    <add key="arbiDirections" value="both"/>
    
    <!-- value = lukuarvo -->
    <add key="maxEurPerTrade" value="100"/>
    <add key="maxEurPerDay" value="1000"/>
    <add key="currencyConversionFeePercent" value="2"/>
    <add key="profitThresholdEur" value="10"/>

  </appSettings>

</configuration>